{
    "name": "interactiveplus/interactivepdk-2020-corelib",
    "description": "Core Support Library for InteractivePlus's New Identity Provider System",
    "require":{
        "php": ">=7.1.0",
        "phpmailer/phpmailer":"~6.1",
        "firebase/php-jwt":"~5.2",
        "thingengineer/mysqli-database-class": "~2.9",
        "giggsey/libphonenumber-for-php": "~8.12",
        "league/oauth2-server": "~8.1",
        "mongodb/mongodb": "~1.6"
    },
    "require-dev":{
        "phpunit/phpunit": "^9"
    },
    "autoload":{
        "psr-4":{
            "InteracitvePlus\\PDK2020Core\\": "src/"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "InteractivePlus\\PDK2020Test\\": "test/"
        }
    }
}